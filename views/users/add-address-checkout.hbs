    <script>

        function validationAddress() {
            console.log("address validsatin called");

            var Name = document.getElementById("inputname").value //1
            var Mobile = document.getElementById("inputMobile").value//2
            var Pincode = document.getElementById("inputPincode").value//3
            var Locality = document.getElementById("inputLocality").value//4
            var Address = document.getElementById("inputAddress").value//5
            var State = document.getElementById("inputState").value//6
            var City = document.getElementById("inputDistrict").value//7
            console.log('name' + Name + "mobile" + Mobile + " pin" + Pincode + "loc" + Locality + "address" + Address + "state" + State + "City" + City)
            var Err = false


            if (!isNaN(Name)) {

                document.getElementById("name").innerHTML =
                    " Enter the valid characters";
                Err = true
            }
            if (Name == "") {
                document.getElementById("name").innerHTML = "Please enter Your name";
                Err = true
            }
            //Mobile
            if (Mobile == "") {
                document.getElementById("mobile").innerHTML = "Please enter Your Phone number";
                Err = true
            }
            if (isNaN(Mobile)) {

                document.getElementById("mobile").innerHTML =
                    " Enter the valid characters";
                Err = true
            }
            if (Mobile.length < 10 || Mobile.length > 10) {
                document.getElementById("mobile").innerHTML =
                    "Please Enter 10 Characters";
                Err = true

            }
            // Pincode
            
            if (isNaN(Pincode)) {

                document.getElementById("Pincode").innerHTML =
                    " Enter the valid characters";
                Err = true
            }
            if (Pincode.length < 6 || Pincode.length > 6) {
                document.getElementById("Pincode").innerHTML =
                    "Please Enter 6 Characters";
                Err = true

            }
            // Locality

            if (!isNaN(Locality)) {

                document.getElementById("Locality").innerHTML =
                    " Enter the valid characters";
                Err = true
            }
            if (Locality == "") {
                document.getElementById("Locality").innerHTML = "Please enter Your Locality";
                Err = true
            }
            //Address

            if (!isNaN(Address)) {

                document.getElementById("Address").innerHTML =
                    " Enter the valid characters";
                Err = true
            }
            if (Address == "") {
                document.getElementById("Address").innerHTML = "Please enter Your Address";
                Err = true
            }
            //State
            if (State == "") {
                document.getElementById("State").innerHTML = "Please Select this field";
                Err = true
            }

            //City
            if (City == "") {
                document.getElementById("City").innerHTML = "Please Select this field";
                Err = true
            }




            if (Err) {
                return false
            }

        }

    </script>


    <script>
        (function ($) {

            "use strict";

            $('nav .dropdown').hover(function () {
                var $this = $(this);
                $this.addClass('show');
                $this.find('> a').attr('aria-expanded', true);
                $this.find('.dropdown-menu').addClass('show');
            }, function () {
                var $this = $(this);
                $this.removeClass('show');
                $this.find('> a').attr('aria-expanded', false);
                $this.find('.dropdown-menu').removeClass('show');
            });

        })(jQuery);

    </script>


