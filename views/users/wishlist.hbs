                            <!-- Google -->
                             <a class="btn  btn-floating m-1" href="#!" role="button"><img width="30px" src="/snipp-cards/maestro.png" alt=""></a>

                            <!-- Instagram -->
                            <a class="btn  btn-floating m-1" href="#!" role="button"><img width="40px" src="/snipp-cards/rupay.png" alt=""></a>


                            <!-- Linkedin -->
                            <a class="btn  btn-floating m-1" href="#!" role="button"><img width="40px" src="/snipp-cards/PP_Acceptance_Marks_for_LogoCenter_266x142.png" alt=""></a>

                            <!-- Github -->
                           <a class="btn  btn-floating m-1" href="#!" role="button"><img width="30px" src="/snipp-cards/download_GnGcaAb.jpg" alt=""></a>

                        </section>
                        <!-- Section: Social media -->
                    </div>
                    <!-- Grid container -->

                    <!-- Copyright -->
                    <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2);">
                        © 2022 Copyright:
                        <a class="text-white" style="text-decoration: none;" href="#">electra@gmail.com</a>
                    </div>
                    <!-- Copyright -->
                </footer>
        <!-- Footer -->
    </div>





