                <main class="row" style="font-family: 'Open Sans Condensed', sans-serif;">
                    <div class="col-lg-4 col-md-6 col-sm-8 mx-auto bg-white py-3 mb-4">
                        <div class="row">
                            <div class="col-12">
                                <form class="font text-success" id="signupform" action="/getotp" method="post">

                                    {{#if otpErr}}
                                    <div class="text-center"><small class="text-danger">{{otpErr}}</small></div>
                                    {{/if}}

                                    {{#if signupErr}}
                                    <div class="text-center"><small class="text-danger">{{signupErr}}</small></div>
                                    {{/if}}

                                    <div class="mb-3">
                                        <label class="font"> Name</label>
                                        <input type="text" class="font form-control" name="Name"
