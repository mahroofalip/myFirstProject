                                    </div>
                                    <div class="mb-3">
                                        <label class="font">Phone</label>
                                        <input type="text" class="form-control font" name="mobile"
                                            placeholder="Enter Mobile">
                                    </div>

                                    <div class="mb-3">
                                        <label class="font">Email address</label>
                                        <input type="email" class="form-control font" name="Email"
                                            placeholder="Enter email">
                                    </div>
                                    <div class="mb-3">


                                    </div>
                                    <div class="mb-3">

                                        <label class="font">Password</label>
                                        <input type="password" class="font form-control" name="Password"
                                            placeholder="Password">

                                        {{#if matchError}}


                                        <div class="text-center text-danger"><small>{{matchError}}</small></div>

                                        {{/if}}
                                        <label class="font" for="password-confirm">Confirm Password</label>
                                        <input type="password" class="font form-control" name="ConformPassword"
                                            placeholder="Password">

                                    </div>

                                    <a style="margin-top: 5px;" class="font text-decoration-none" href="/login"> Already
                                        have
                                        account ?</a>
                                    <button style="float: right;" type="submit"
                                        class="font btn btn-primary float-right">Signup</button>
                                </form>
                            </div>


                        </div>
                    </div>
                </main>
